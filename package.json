{
  "name": "cr-manager-back",
  "version": "1.0.0",
  "description": "Application to manage and organize GitHub code reviews",
  "main": "index.js",
  "scripts": {
    "start": "tsc && node ./build/index.js",
    "dev": "nodemon -L --watch './src/**/*.ts' --exec \"ts-node\" ./src/index.ts",
    "format": "prettier --write \"src/**/*.{js,ts,json}\"",
    "lint": "eslint \"src/**/*.{js,ts}\" --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ilchampo/cr-manager-back.git"
  },
  "keywords": [
    "github",
    "code",
    "review",
    "developers",
    "application",
    "organize",
    "manage"
  ],
  "author": "Pablo Beltran",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ilchampo/cr-manager-back/issues"
  },
  "homepage": "https://github.com/Ilchampo/cr-manager-back#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.6.2",
    "mongoose": "^8.4.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mongodb": "^4.0.7",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.1",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
